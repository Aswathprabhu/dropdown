<div>
    <div>
    {{input
        value=headVal
        click=(action "setArray" listobj)
        size=100
    }}
    </div>
    <div style="border-width:3px 3px 3px 3px ;" >
        {{#if dropdownBool}}
            <div>
                {{input
                    id="filterDrop"
                    value=filterVal
                    size=100
                    keyUp=(action "filterByValue")   
                }}
            </div>
            <div style="border-style: solid;width:603px">
                {{#each listObj as |list|}}
                    <div class="listDropDown" onclick={{action "setHeadVal" list.name}}>{{list.name}}</div>
                {{/each}}
            </div>
        {{/if}}
    </div>
</div>
{{yield headVal}}